<?xml version="1.0"?>
<package>
  <metadata>
    <id>SnippetManager</id>
    <version>1.3.1</version>
    <title>Snippet Manager</title>
    <authors>Josef Pihrt</authors>
    <owners>Josef Pihrt</owners>
    <licenseUrl>https://github.com/JosefPihrt/SnippetManager/blob/master/LICENSE.txt</licenseUrl>
    <projectUrl>https://github.com/JosefPihrt/SnippetManager</projectUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Snippet Manager API enables to work with Visual Studio snippets.</description>
    <summary>Snippet Manager API enables to work with Visual Studio snippets.</summary>
    <copyright>Copyright (c) 2015-2017 Josef Pihrt</copyright>
    <tags>Snippet Snippets CodeSnippet</tags>
    <dependencies>
      <group targetFramework=".NETFramework4.0" />
      <group targetFramework=".NETPortable4.5-Profile259" />
      <group targetFramework=".NETStandard1.3">
        <dependency id="System.Collections" version="4.3.0" />
        <dependency id="System.Diagnostics.Debug" version="4.3.0" />
        <dependency id="System.IO" version="4.3.0" />
        <dependency id="System.IO.FileSystem" version="4.3.0" />
        <dependency id="System.Linq" version="4.3.0" />
        <dependency id="System.Runtime.Extensions" version="4.3.0" />
        <dependency id="System.Text.Encoding.Extensions" version="4.3.0" />
        <dependency id="System.Text.RegularExpressions" version="4.3.0" />
        <dependency id="System.Xml.XmlSerializer" version="4.3.0" />
      </group>
    </dependencies>
  </metadata>
  <files>
    <file src="*.dll;*.xml" target="lib\net40" exclude="*.pdb" />
    <file src="Portable\*.dll;Portable\*.xml" target="lib\portable45-net45+win8+wp8+wpa81" exclude="*.pdb" />
    <file src="NETStandard\*.dll;NETStandard\*.xml" target="lib\netstandard1.3" exclude="*.pdb" />
  </files>
</package>